//Generic Constraints : means without Constraints it can be any type <T> its like a placeholder than can be replaced by anything

class GenericConstraints
{
    class Example2 (){ }
    
    interface IExample3() { }
    
    
    class Example<T, U> where T : Example2, IExample3
                        where U : Example2
    {
        
        public static void Main(string[] args) {
            
        }
        
    }
}
